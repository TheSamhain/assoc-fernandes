{
  "hosting": {
    "ignore": [
      "firebase.json",
      "**/.*",
      "/node_modules/**"
    ],
    "public": "dist",
    "rewrites": [
      {
        "destination": "/index.html",
        "source": "**"
      }
    ],
    "target": "afkarate"
  },
  "storage": {
    "public": "public",
    "rules": "storage.rules",
    "site": "afkarate"
  }
}