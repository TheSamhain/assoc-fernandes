{
  "expo": {
    "name": "afkarate",
    "slug": "afkarate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "afkarate",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",

      "backgroundColor": "#FFFFFF",
      "barStyle": "default",
      "name": "Associação Fernandes de Karatê-do",
      "shortName": "Assoc. Fernandes Karatê",
      "description": "Site para Associação Fernandes de Karatê",
      "display": "fullscreen",
      "lang": "pt-BR",
      "orientation": "portrait",
      "scope": "https://afkarate.web.app/",
      "themeColor": "#FFFFFF"
    },
    "extra": {
      "eas": {
        "projectId": "c2b23f56-26f9-4ebc-9a13-e609f7aec1b9"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "O app precisa de acesso as suas fotos para publicar eventos"
        }
      ]
    ]
  }
}
